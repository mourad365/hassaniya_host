# Production Environment Configuration for hassaniya.info
# WARNING: Never commit this file with actual production values
# Use Vercel dashboard or CLI to set these environment variables

# Supabase Configuration (REQUIRED)
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-supabase-anon-key

# Facebook Integration
VITE_FACEBOOK_PAGE_ID=101470072189930
VITE_FACEBOOK_PAGE_TOKEN=your_production_facebook_page_access_token

# Bunny CDN Configuration (REQUIRED)
VITE_BUNNY_STORAGE_API_KEY=your-production-bunny-storage-api-key
VITE_BUNNY_STORAGE_ZONE=your-production-storage-zone-name
VITE_BUNNY_CDN_URL=https://your-production-zone.b-cdn.net

# Bunny Video Library Configuration (REQUIRED)
VITE_BUNNY_VIDEO_LIBRARY_ID=493708
VITE_BUNNY_VIDEO_CDN_HOSTNAME=vz-a9578edc-805.b-cdn.net
VITE_BUNNY_VIDEO_API_KEY=your-production-bunny-video-api-key

# File Upload Configuration
VITE_ENABLE_FILE_UPLOAD=true
VITE_MAX_FILE_SIZE=200000000

# Social Media Links
VITE_SOCIAL_FACEBOOK_URL=https://facebook.com/yourproductionpage
VITE_SOCIAL_TWITTER_URL=https://twitter.com/yourhandle
VITE_SOCIAL_INSTAGRAM_URL=https://instagram.com/yourhandle
VITE_SOCIAL_YOUTUBE_URL=https://youtube.com/@yourchannel

# Production Settings
NODE_ENV=production
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0

# Security Settings (DO NOT CHANGE)
VITE_ENABLE_DEBUG=false
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_SOURCE_MAPS=false
